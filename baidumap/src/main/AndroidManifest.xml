<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.baidumap">


    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
    <uses-permission android:name="android.permission.READ_LOGS"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:supportsRtl="true">

        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.baidu.location.service_v2.2"></action>
            </intent-filter>
        </service>
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="cWHQhgSjchpI6j4LoXkwFfjDj8G8m8RI" />  <!--http://lbsyun.baidu.com/apiconsole/key-->

        <activity android:name="com.hl.baidu.LocationActivity"></activity>
        <activity android:name="com.hl.baidu.LocationOption"></activity>
        <activity android:name="com.hl.baidu.LocationFilter"></activity>
        <activity android:name="com.hl.baidu.NotifyActivity"></activity>
        <activity android:name="com.hl.baidu.QuestActivity"></activity>
        <activity android:name="com.hl.baidu.LocationAutoNotify"></activity>
        <activity android:name="com.hl.baidu.IndoorLocationActivity"></activity>
        <activity android:name="com.hl.baidu.IsHotWifiActivity"></activity>
    </application>

</manifest>
